<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/white" /> <!-- Background color for the circle -->
        </shape>
    </item>
    <item android:top="2dp" android:bottom="2dp" android:left="2dp" android:right="2dp">
        <bitmap
            android:gravity="center"
            android:src="@drawable/ic_logo" /> <!-- Replace with your actual logo -->
    </item>
</layer-list>
